{
  "name": "paulbunyannet/gigazone-gaming",
  "description": "Gigazone Gaming website",
  "authors": [
    {
      "name": "Nate Nolting",
      "email": "naten@paulbunyan.net"
    }
  ],
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "wordpress",
        "type": "webroot",
        "version": "4.4.2",
        "dist": {
          "type": "zip",
          "url": "https://github.com/WordPress/WordPress/archive/4.4.2.zip"
        },
        "require": {
          "fancyguy/webroot-installer": "1.0.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "pmenard/media-tags",
        "type": "wordpress-plugin",
        "version": "3.2.0.2",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/plugin/media-tags.3.2.0.2.zip"
        },
        "require": {
          "composer/installers": "v1.0.7"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "nacin/link-manager",
        "type": "wordpress-plugin",
        "version": "0.1-beta",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/plugin/link-manager.zip"
        },
        "require": {
          "composer/installers": "v1.0.7"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "archetyped/cornerstone",
        "type": "wordpress-plugin",
        "version": "0.7.5",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/plugin/cornerstone.zip"
        },
        "require": {
          "composer/installers": "v1.0.7"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "mungobbq/enable-media-replace",
        "type": "wordpress-plugin",
        "version": "3.0.4",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/plugin/enable-media-replace.zip"
        },
        "require": {
          "composer/installers": "v1.0.7"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.2.*",
    "wordpress": "4.*",
    "fancyguy/webroot-installer": "1.0.0",
    "symfony/yaml": "~2.4",
    "paulbunyannet/autoversion": "~1.0",
    "jarednova/timber": "^0.22.5",
    "pmenard/media-tags": "3.2.0.2",
    "nacin/link-manager": "0.1-beta",
    "paulbunyannet/bandolier": "^1.0",
    "twig/twig": "^1.24",
    "cocur/slugify": "^1.4",
    "archetyped/cornerstone": "0.7.5",
    "anahkiasen/underscore-php": "^2.0",
    "mungobbq/enable-media-replace":"3.0.4",
    "tedivm/stash": "^0.14.1",
    "vlucas/phpdotenv": "^2.2"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "symfony/css-selector": "2.8.*|3.0.*",
    "symfony/dom-crawler": "2.8.*|3.0.*"
  },
  "extra": {
    "webroot-dir": "public_html/wp",
    "webroot-package": "wordpress",
    "installer-paths": {
      "public_html/wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "public_html/wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    }
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "files": [
      "src/Wordpress/functions.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Wordpress\\": "src/Wordpress/",
      "GigaZone\\": "src/GigaZone/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
